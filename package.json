{
  "name": "twitter-archive-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/node": "^14.14.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/uuid": "^8.3.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "moment": "^2.29.1",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-infinite-scroll-component": "^5.1.0",
    "react-redux": "^7.2.1",
    "react-scripts": "^4.0.0",
